        <Command name="XmlFormat" Ctrl="no" Alt="no" Shift="no" Key="0">cmd /c cd /d $(CURRENT_DIRECTORY)&amp;xmlint -format $(FILE_NAME) -o $(FILE_NAME)&amp;pause</Command>
