    <UserDefinedCommands>
        <Command name="TccRun" Ctrl="no" Alt="no" Shift="no" Key="115">cmd /c cd /d $(CURRENT_DIRECTORY)&amp;tcc -run $(FILE_NAME)&amp;pause</Command>
        <Command name="GccRun" Ctrl="no" Alt="no" Shift="no" Key="0">cmd /c clang $(FULL_CURRENT_PATH) -o $(CURRENT_DIRECTORY)\$(NAME_PART).exe&amp; $(CURRENT_DIRECTORY)\$(NAME_PART) &amp; pause</Command>
